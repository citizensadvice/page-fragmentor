{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": [
    "babel"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "import/no-default-export": "error",
    "no-restricted-syntax": "off",
    "no-continue": "off"
  }
}
